{
  "productName": "tauri-app",
  "version": "0.0.0",
  "identifier": "com.tauri.devs",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:4321/",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "File Explorer",
        "label": "main",
        "width": 800,
        "height": 600
      },
      {
        "title": "Demo Information",
        "label": "demo",
        "width": 800,
        "height": 600,
        "url": "/demo"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
